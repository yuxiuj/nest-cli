{
  "name": "tester-pro",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "start:local": "ts-node  src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node dist/main.js",
    "generate-graphql-typings": "ts-node src/generate-typings.ts",
    "lint": "tslint -p tsconfig.json -c tslint.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "tslint --fix"
    ]
  },
  "dependencies": {
    "@godaddy/terminus": "^4.2.0",
    "@nestcloud/common": "^0.5.2",
    "@nestjs/common": "^6.10.1",
    "@nestjs/core": "6.7.1",
    "@nestjs/graphql": "6.5.1",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "6.7.1",
    "@nestjs/terminus": "^6.5.1",
    "@nestjs/typeorm": "6.1.3",
    "apollo-server-express": "2.9.3",
    "assert": "^2.0.0",
    "cookie-parser": "^1.4.4",
    "etcd3": "^0.2.13",
    "global": "^4.4.0",
    "graphql": "14.5.6",
    "graphql-subscriptions": "1.1.0",
    "ioredis": "^4.14.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mysql": "^2.17.1",
    "nest-winston": "1.1.4",
    "prettier": "^1.19.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.3",
    "typeorm": "^0.2.18",
    "winston": "^3.2.1",
    "jaeger-client": "^3.17.1",
    "prom-client": "^11.5.3",
    "kafkajs": "^1.11.0",
    "jison": "^0.4.18",
    "require-in-the-middle": "^5.0.2"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.10",
    "@nestjs/testing": "^6.7.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "10.14.18",
    "@types/redis": "^2.8.14",
    "@types/restify": "7.2.12",
    "mocha": "^6.2.0",
    "nodemon": "~1.19.1",
    "nyc": "^14.1.1",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "husky": "3.0.4",
    "typescript": "3.7.2",
    "lint-staged": "^9.4.2",
    "@commitlint/cli": "8.1.0",
    "wrk": "^1.2.0"
  }
}
